<script>
  document.addEventListener('DOMContentLoaded', function () {
      const menuItems = document.querySelectorAll('.menu-item');

      // Function to remove 'active' class from all menu items
      function removeActiveClass() {
          menuItems.forEach(function (item) {
              item.classList.remove('active');
          });
      }

      // Function to set the active menu item in local storage
      function setActiveMenuItem(item) {
          localStorage.setItem('activeMenuItem', item.href);
      }

      // Function to get the active menu item from local storage
      function getActiveMenuItem() {
          return localStorage.getItem('activeMenuItem');
      }

      // Add click event listener to each menu item
      menuItems.forEach(function (item) {
          item.addEventListener('click', function (event) {
              // Remove 'active' class from all menu items
              removeActiveClass();

              // Add 'active' class to the clicked menu item
              item.classList.add('active');

              // Set the active menu item in local storage
              setActiveMenuItem(item);
          });
      });

      // Check if there's an active menu item stored in local storage on page load
      const activeMenuItem = getActiveMenuItem();
      if (activeMenuItem) {
          const menuItem = document.querySelector(`.menu-item a[href="${activeMenuItem}"]`);
          if (menuItem) {
              menuItem.closest('.menu-item').classList.add('active');
          }
      }
  });
</script>



