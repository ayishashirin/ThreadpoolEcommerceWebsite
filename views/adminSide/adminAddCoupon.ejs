    <!-- header -->
    <%- include('../includes/adminSide/header') %>
    <!-- end of header -->
    
    <!-- My CSS -->
    <link rel="stylesheet" href="/adminSide/css/adminAddCoupon.css" />
  
    <!-- CONTENT -->
    <section id="content">

      <!-- MAIN -->
      <main>
        <div class="head-title">
          <div class="left">
            <h1>Add Coupon</h1>
            <ul class="breadcrumb">
              <li>
                <a href="/adminHome">Dashboard</a>
              </li>
              <li><i class="bx bx-chevron-right"></i></li>
              <li>
                <a href="/adminCouponManagement">Coupon Management</a>
              </li>
              <li><i class="bx bx-chevron-right"></i></li>
              <li>
                <a class="active" href="/adminAddCoupon">Add Coupon</a>
              </li>
            </ul>
          </div>
        </div>

        <div class="manageOptions">
          <a href="/adminCouponManagement">
            <button>Go Back</button>
          </a>
        </div>

        <div class="table-data justify-content-center">
          <div id="addProduct" class="order">
            <form class="addProductForm" action="/adminAddCoupon" method="post">
                <div class="d-flex align-items-center formRow">
                    <div class="w-50 <%= errMesg?.code?'errDiv':'' %>">
                        <label for="code" class="d-block">Coupon Code</label>
                        <input type="text" name="code" value="<%= savedDetails?.code?savedDetails.code:'' %>">
                        <p class="err"><%= errMesg?.code %></p>
                    </div>
                    <div class="w-50">
                        <label for="Category" class="d-block">Category</label>
                        <select name="category">
                          <option value="All">All</option>
                          <% for( let i = 0; i < category.length; i++ ) { %>
                              <option class="text-capitalize" value="<%= category[i].name %>" <%= (savedDetails?.category === category[i].name)?'selected':'' %>><%= category[i].name %></option>
                          <% } %>
                        </select>
                    </div>
                </div>

                <div class="d-flex align-items-center formRow">
                    <div class="w-50 <%= errMesg?.discount?'errDiv':'' %>">
                        <label for="discount" class="d-block">Coupon Discount</label>
                        <input type="number" name="discount" value="<%= savedDetails?.discount?savedDetails.discount:'' %>">
                        <p class="err"><%= errMesg?.discount %></p>
                    </div>
                    <div  class="dheading w-50 <%= errMesg?.count?'errDiv':'' %>">
                        <label for="count" class="d-block">Max uses</label>
                        <input type="number" name="count" value="<%= savedDetails?.count?savedDetails.count:'' %>">
                        <p class="err"><%= errMesg?.count %></p>
                    </div>
                </div>

                <div class="d-flex align-items-center formRow">
                    <div class="w-50 <%= errMesg?.minPrice?'errDiv':'' %>">
                        <label for="minPrice" class="d-block">Use Above</label>
                        <input type="number" name="minPrice" value="<%= savedDetails?.minPrice?savedDetails?.minPrice:'' %>">
                        <p class="err"><%= errMesg?.minPrice %></p>
                    </div>
                    <div class="w-50 <%= errMesg?.expiry?'errDiv':'' %>">
                      <label for="expiry" class="d-block">Coupon Expiry</label>
                      <input type="date" name="expiry" value="<%= savedDetails?.expiry?savedDetails?.expiry:'' %>">
                      <p class="err"><%= errMesg?.expiry %></p>
                  </div>
                </div>

                <div class="d-flex justify-content-center">
                    <button type="submit" class="addBtn" id="addBtn">Add Coupon</button>
                </div>
            </form>
          </div>
        </div>
      </main>
      <!-- MAIN -->
    </section>
    <!-- CONTENT -->

    <script src="/adminSide/js/adminAddCoupon.js"></script>

    <!-- fontawesome -->
    <script
      src="https://kit.fontawesome.com/23d1247997.js"
      crossorigin="anonymous"
    ></script>
    <!-- end of fontawesome -->
    <!-- Bootstrap script file -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
    <!--end of Bootstrap script file -->
  </body>
</html>
